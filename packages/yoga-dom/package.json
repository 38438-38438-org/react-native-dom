{
  "name": "yoga-dom",
  "description": "WASM bindings for Yoga Layout",
  "version": "0.0.10",
  "license": "MIT",
  "author": "Vincent Riemer",
  "main": "lib/Yoga.cjs.js",
  "module": "lib/Yoga.es.js",
  "files": ["READNE.md", "dist"],
  "scripts": {
    "clean:bundle": "rimraf dist",
    "compile:wasm": "./build.sh",
    "compile:min": "rollup --silent -c rollup.config.umd.min.js",
    "compile:es": "rollup --silent -c rollup.config.es.js",
    "compile:umd": "rollup --silent -c rollup.config.umd.js",
    "compile:commonjs": "rollup --silent -c rollup.config.cjs.js",
    "precompile:bundle": "npm run clean:bundle",
    "compile:bundle":
      "npm run compile:umd && npm run compile:commonjs && npm run compile:es && npm run compile:min",
    "compile": "npm run compile:wasm && npm run compile:bundle",
    "bench": "node tests/run-bench $(find tests/Benchmarks -name '*.js')"
  },
  "devDependencies": {
    "rimraf": "^2.6.2",
    "rollup": "^0.53.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-uglify": "^2.0.1",
    "uglify-es": "^3.3.4"
  }
}
